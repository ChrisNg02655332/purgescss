{
  "name": "purgescss",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "release": "release-it"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "6",
    "sass": "^1.43.5",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "@commitlint/config-conventional": "^15.0.0",
    "@release-it/conventional-changelog": "release-it/conventional-changelog",
    "commitlint": "^15.0.0",
    "husky": "^7.0.4",
    "release-it": "^14.11.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
